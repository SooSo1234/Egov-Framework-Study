<sqlMap namespace="DeptSpace">

	<typeAlias  alias="egovMap" type="org.egovframe.rte.psl.dataaccess.util.EgovMap"/>
	<typeAlias  alias="deptVO" type="egovframework.example.sample.service.DeptVO"/>

	<insert id="deptDAO.insertDept">
		insert into dept(deptno,dname,loc) values(#deptno#,#dname#,#loc#)
	</insert>

	<select id="deptDAO.selectDeptList" resultMap="egovMap">
		select deptno, dname, loc from dept
	</select>




	<select id="deptDAO.select">
		select drivername from parkingmanager where parkingspot = '1'
	</select>

